QWidget{
    background-color: #2a2a2a;
    color: #B7B7B7 ;
}

/* Module Panel Buttons */
QPushButton{
    background-color: #434343; 
    border: 2px #434343; 
    border-radius: 3px; 
    min-height: 20px; 
    color: #B7B7B7
}

QTabBar#PrimaryTabBar::tab{
    height: 30px;
    margin: 0px;
    padding: 0px;
    font: 14px;

}

/* Tab button on the top, active */
QTabBar#PrimaryTabBar::tab:selected{
    background-color: #2a2a2a; 
    border:none; 
    border-top: 4px solid #5ac2c9;
    border-bottom: 2px solid #2a2a2a;
    min-height: 20px; 
    min-width: 200px;
    font: bold 14px;
    color: #5ac2c9
} 

/* Tab button on the top, inactive */
QTabBar#PrimaryTabBar::tab:!selected{
    background-color: #121212; 
    border:none; 
    border-top: 4px solid #121212;
    border-bottom: 2px solid #434343;
    min-height: 20px; 
    min-width: 200px;

    color: #FAFAFA
}  

QTabBar#SecondaryTabBar::tab{
    height: 30px;
    margin: 0px;
    padding: 0px;
    font: 10px;
    background-color: #2a2a2a;
    min-width: 200px;

}

/* Tab button on the top, active */
QTabBar#SecondaryTabBar::tab:selected{
    color: #5ac2c9; 
    
} 

/* Tab button on the top, inactive */
QTabBar#SecondaryTabBar::tab:!selected{
    color: #B7B7B7; 
    
}


